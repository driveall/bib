@inproceedings{Weng2011,
address = {Portland, Oregon},
annote = {Presents an approach to summarising individual posts in microblogs using the responses to them.

Posts are characterised as interrogation, sharing, discussion and chat, and the summarisation depends on that classification. Intention classification is performed using a 6-gram classification model along with some other features such as the precense of URLs and file pointers.

Conversations are summarised by classifying response pairs - where one message specifically responds to a previous one. These sets of response pairs are grouped to identify different topics within the same conversation.

The relevancy of responses to questions is also detected. Positive/negative sentiment is extracted when the user has shared a URL, and a pie chart is created from the results.

An interactive search system is developed which displays these summaries when a post is clicked.
      },
author = {Weng, J. Y. and Yang, C. L. and Chen, B. N. and Wang, Y. K. and Lin, S. D.},
