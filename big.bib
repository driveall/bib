@inproceedings{Becker2011a,
abstract = {User-contributed messages on social media sites such as Twitter have emerged aspowerful, real-time means of information sharing on the Web. These short messages tend to reflect a variety of events in real time, making Twitter particularly well suited as a source of real-time event content. In this paper, we explore approaches for analyzing the stream of Twitter messages to distinguish between messages about real-world events andnon-event messages. Our approach relies on a rich family of aggregatestatistics of topically similar message clusters. Large-scale experiments over millions of Twitter messages show the effectiveness of our approach for surfacing real-world event content on Twitter.},
annote = {This approach concentrates on detecting events. A real time clustering algorithm is used to form communities of tweets, then a classifier is trained to discover which of those communities correspond to events.

Novel features are used such as volume of tweets over time, as well as social features like the types and quantity user interactions, and the topics discussed (such as the number of key words which appear).

This paper contains some useful outline discussion of the issues relating to the diversity of tweets and the definition of events.},
author = {Becker, Hila and Naaman, Mor and Gravano, Luis},
title = {{ Beyond Trending Topics: Real-World Event Identification on Twitter }},
booktitle = {Proceedings of the Fifth International Conference on Weblogs and Social Media (ICWSM)},
year = {2011}
}

@inproceedings{Becker2011b,
abstract = {Social media sites such as Twitter contain large amounts of user contributed messages for a wide variety of real-world events. While some of these "event messages" might contain interesting and useful information (e.g., event time, location, participants, opinions), others might provide little value (e.g., using heavy slang, incomprehensible language) to people interested in learning about an event. Techniques for effective selection of quality event content may therefore help improve applications such as event browsing and search.In this paper, we explore approaches for finding representative messages among a set of Twitter messages that correspond to the same event, with the goal of identifying high quality, relevant messages that provide useful event information. We evaluate our approaches using a large-scale dataset of Twitter messages, and show that we can automatically select event messages that are both relevant and useful.},
annote = {This continues the work described in "Beyond Trending topics" - tweets are selected from the clusters based on three different measures. The measures are distance to centroid, degree centrality and LexRank centrality. It was found that the cosine distance to the centroid seemed to be the "best" way to select content, though cannot rule out that all approaches work equally well.

The evaluation assumed that selected tweets must have the attributes of quality, relevance and usefulness. Human annotators were asked to assign scores to the top-5 tweets according to these attributes. They claimed that annotators agree strongly on the scores.},
author = {Becker, H. and Naaman, M. and Gravano, L.},
title = {{ Selecting Quality Twitter Content for Events }},
booktitle = {Proceedings of the Fifth International Conference on Weblogs and Social Media (ICWSM)},
url = {http://www.aaai.org/ocs/index.php/ICWSM/ICWSM11/paper/view/2744},
year = {2011}
}

